VAR i,j:integer;

BEGIN

i:= 0;
repeat
  write(i,' ');
  i := succ(i);
  j := 1;
  repeat
    write(j,' ');
    j := succ(j);
    if j = 3 then
      write("stopping at ",j);
      stop;
      write("stopped j");
    end;
  until j > 5;
  if i = 10 then
    write("\nstopping at ",i);
    stop;
    write("stopped i");
  end;
  write('\n');
until i > 20;

END.
